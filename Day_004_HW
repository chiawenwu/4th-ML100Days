import os
import numpy as np
import pandas as pd
dir_data='./data/'
f_app = os.path.join(dir_data,'application_train.csv')
print('Path of read in data: %s' % (f_app))
app_train = pd.read_csv(f_app)
print('列出所有欄位')
print(app_train)
print('列出欄位資料')
print(app_train.info())
print('截取部份資料前10筆')
print(app_train.tail(10))
